@model List<MVCExample.Models.Customer>
@{
    ViewBag.Title = "CustomerList";
}

<h2>CustomerList</h2>
<table>
    <tr>
        <th>ID</th>
        <th>Customer Name</th>
        <th>Address</th>
        <th>Phone Number</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
            <td>@item.Address</td>
            <td>@item.Phone</td>
        </tr>
    }
</table>
